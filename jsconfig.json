{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@@/*": [
        "./*"
      ],
      "@/*": [
        "src/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    },
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}